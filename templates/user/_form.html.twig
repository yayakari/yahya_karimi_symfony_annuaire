{{ form_start(form) }}
<div class="row col-md-6 m-auto">
	{{ form_row(form.email) }}
    {{ form_row(form.plainPassword, {
            label: 'Password'
        }) }}
	{{ form_row(form.nom) }}
	{{ form_row(form.prenom) }}
	{{ form_row(form.secteur) }}
	{{ form_row(form.contrat) }}
	{{ form_row(form.sortie) }}
	{{ form_row(form.photo) }}
	{{ form_row(form.roles) }}
	<button class="btn btn-primary">{{ button_label|default('Enregistrer') }}</button>
</div>
{{ form_end(form) }}
